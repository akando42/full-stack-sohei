{
  "name": "destr",
  "version": "1.0.0",
  "description": "A faster, secure and convenient alternative for JSON.parse",
  "repository": "nuxt-contrib/destr",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
