    "test": "yarn lint"
  },
  "devDependencies": {
    "@hapi/bourne": "^2.0.0",
    "@nuxtjs/eslint-config-typescript": "latest",
    "benchmark": "latest",
    "bili": "latest",
    "eslint": "latest",
    "rollup-plugin-typescript2": "latest",
    "secure-json-parse": "^2.1.0",
    "standard-version": "latest",
    "typescript": "latest"
  }
